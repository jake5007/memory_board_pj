{
  "name": "memory_board",
  "version": "1.0.0",
  "description": "memory board mini project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon server/index.js",
    "client": "cd client && npm start",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node server/seeder.js",
    "data:destroy": "node server/seeder.js -d",
    "build": "npm install && npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jake5007/project_memory_board.git"
  },
  "author": "Hyunjin Chae",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jake5007/project_memory_board/issues"
  },
  "homepage": "https://github.com/jake5007/project_memory_board#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1"
  }
}
